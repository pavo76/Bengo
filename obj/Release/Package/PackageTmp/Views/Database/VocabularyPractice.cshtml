@model IEnumerable<Bengo.Models.Vocabulary_Practice>

@{
    ViewBag.Title = "VocabularyPractice";
}

<h2>VocabularyPractice</h2>

<p>
    @Html.ActionLink("Create New", "Create", "Vocabulary_Practice")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vocabulary.VocabularyUnit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastPracticed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RepeatInterval)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vocabulary.VocabularyUnit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastPracticed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RepeatInterval)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
